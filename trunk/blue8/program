6008    // (000)start:	lda thevar
1009    // (001)loop:	add sum
200a    // (002)	 xor flip
0002    // (003)	not
0005    // (004)	inca
0006    // (005)	deca
7008    // (006)	sta thevar  		; this is wasteful, but tests
a000    // (007)	jmp start
0012    // (008)thevar:	DW value
0004    // (009)sum:	DW offset*2
ffff    // (00a)flip:	DW 0xFFFF	
// Symbols
// thevar: 0008
// value: 0012
// flip: 000a
// sum: 0009
// loop: 0001
// start: 0000
// offset: 0002
// End Symbols
